"*":
  "clang-format":
    formatCOnSave: true
    formatCPlusPlusOnSave: true
  core:
    projectHome: "/home/latino/src/"
    telemetryConsent: "limited"
    themes: [
      "atom-dark-ui"
      "one-dark-syntax"
    ]
  editor:
    fontFamily: "Fira Mono"
    invisibles: {}
    tabLength: 4
  "exception-reporting":
    userId: "42052f43-3441-4ef3-82f4-65fb8b1b4580"
  "linter-gcc":
    gccDefaultCFlags: "-c -Wall -std=c11"
    gccSuppressWarnings: true
  minimap:
    charHeight: 3
    charWidth: 1.5
    plugins:
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
  welcome:
    showOnStartup: false
  "you-complete-me":
    globalExtraConfig: "~/.vim/.ycm_extra_conf.py"
    ycmdPath: "/home/latino/.vim/bundle/YouCompleteMe/third_party/ycmd"
