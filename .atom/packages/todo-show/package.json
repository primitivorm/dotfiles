        "XXX",
        "IDEA",
        "HACK",
        "NOTE",
        "REVIEW",
        "NB",
        "BUG",
        "QUESTION",
        "COMBAK",
        "TEMP"
      ],
      "items": {
        "type": "string"
      }
    },
    "findUsingRegex": {
      "description": "Regex string used to find all your todos. `${TODOS}` is replaced with `FindTheseTodos` from above.",
      "type": "string",
      "default": "/\\b(${TODOS})[:;.,]?\\d*($|\\s.*$|[\\{\\[\\(].+$)/g"
