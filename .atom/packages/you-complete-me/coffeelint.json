{
  "arrow_spacing": {
    "level": "error"
  },
  "no_trailing_whitespace": {
    "allowed_in_empty_lines": false
  },
  "max_line_length": {
    "level": "ignore"
  },
  "colon_assignment_spacing": {
    "level": "error",
    "spacing": {
      "left": 0,
      "right": 1
    }
  },
  "no_empty_param_list": {
    "level": "error"
  },
  "no_stand_alone_at": {
    "level": "error"
  },
  "space_operators": {
    "level": "error"
  },
  "empty_constructor_needs_parens": {
    "level": "error"
  },
  "no_unnecessary_fat_arrows": {
    "level": "error"
  },
  "no_unnecessary_double_quotes": {
    "level": "error"
  },
  "no_interpolation_in_single_quotes": {
    "level": "error"
  },
  "prefer_english_operator": {
    "level": "error",
    "doubleNotLevel": "error"
  },
  "spacing_after_comma": {
    "level": "error"
  },
  "ensure_comprehensions": {
    "level": "ignore"
  }
}
