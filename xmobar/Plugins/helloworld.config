Config { font = "-misc-fixed-*-*-*-*-10-*-*-*-*-*-*-*"
       , bgColor = "#000000"
       , fgColor = "#BFBFBF"
       , position = TopW C 90
       , commands = [ Run Cpu [] 10
                    , Run Weather "LIPB" [] 36000
                    , Run HelloWorld
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%cpu% } %helloWorld% { %LIPB% | <fc=yellow>%date%</fc>"
       }
